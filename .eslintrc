{
    "extends": ["airbnb-base"],
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "max-len": ["error", 120],
        "strict": 0,
        "no-await-in-loop": 0,
        "no-restricted-syntax": 0
    },
    "globals": {
        "beforeEach": true,
        "describe": true,
        "expect": true,
        "it": true
    }
}